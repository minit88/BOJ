    "n=int(input())\n",
    "\n",
    "s=0\n",
    "e=n\n",
    "while s<=e:\n",
    "    mid = (s+e)//2\n",
    "    if mid **2<n:\n",
    "        s=mid+1\n",
    "    else:\n",
    "        e=mid-1\n",
    "print(s)"
