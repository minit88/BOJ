      "source": [
        "n,m =map(int,input().split())\n",
        "\n",
        "s,e = 1,n\n",
        "ans = 1000000000000\n",
        "origin_vic= m*100//n\n",
        "\n",
        "while s<=e:\n",
        "  mid = (s+e)//2\n",
        "  vic = (m+mid)*100//(n+mid)\n",
        "\n",
        "  if vic>origin_vic :\n",
        "    ans = min(ans,mid)\n",
        "    e=mid-1\n",
        "  else:\n",
        "    s=mid+1\n",
        "if ans == 1000000000000:\n",
        "  print(-1)\n",
        "else:\n",
        "  print(ans)\n",
        "\n"
      ]
    }
  ]
