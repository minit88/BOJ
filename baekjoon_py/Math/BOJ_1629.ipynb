      "source": [
        "a,b,c = map(int,input().split())\n",
        "def solve(a,b,c):\n",
        "  if b==1:\n",
        "    return a%c\n",
        "\n",
        "  elif b%2 == 0:\n",
        "    return solve(a,b//2,c)**2%c\n",
        "\n",
        "  else:\n",
        "    return (solve(a,b//2,c)**2)*a%c\n",
        "\n",
        "\n",
        "print(solve(a,b,c))"
      ]
    }
  ]
