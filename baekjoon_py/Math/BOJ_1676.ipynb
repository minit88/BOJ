   "source": [
    "from math import factorial\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "result = 0\n",
    "processedFactorial = list(map(int, str(factorial(N))))\n",
    "processedFactorial.reverse()\n",
    "\n",
    "for i in range(N):\n",
    "    if processedFactorial[i]== 0 :\n",
    "        result+=1\n",
    "    else :\n",
    "        break\n",
    "print(result)"
   ],
